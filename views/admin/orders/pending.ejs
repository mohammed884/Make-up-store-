<%- include("../../layout/admin-navbar") -%>
<% if(suc.length > 0){%>
    <script>
        Swal.fire(
            'تم قبول الطلب',
            'تم نقل الطلب الى قائمة الطلبات المقبولة',
            'success'
        )
    </script>
<%} %>

<% if(reject.length > 0){%>
    <script>
        Swal.fire(
            'تم رفض الطلب',
            'تم نقل الطلب الى قائمة الطلبات المرفوضة',
            'success'
        )
    </script>
<%} %>
<p class="order-title">الطلبات الجديدة</p>
    <div class="pending-orders">
        <br>
        <% orders.forEach(data => {%>
            <div class="folders" onclick="window.location.href='/admin/bills/get/<%= data.id%>'">
                <ion-icon class="folder icon" name="folder"></ion-icon>
                <p><%= data.name%></p>
            </div>
        <%}) %>
    </div>
    <%- include("../../layout/footer") -%>